    "clear": "rm -rf src/components src/features",
    "publish:npm": "npm publish --access=public"
  },
  "author": "Harsh Rohila",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "pkgroll": "^2.4.2",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "zx": "^8.1.5"
  },
  "files": [
    "dist/**/*",
    "templates/**/*"
  ],
  "keywords": [
    "cli",
    "command-line",
    "tool",
    "react"
  ],
  "volta": {
    "node": "20.17.0"
  }
