{
  "name": "@rohilaharsh/wgen",
  "version": "1.0.5",
  "description": "A CLI tool for generating React.js files",
  "main": "dist/index.js",
  "bin": {
    "wgen": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HarshRohila/gen-react.git"
  },
  "type": "module",
  "scripts": {
    "test": "jest",
    "build": "pkgroll",
